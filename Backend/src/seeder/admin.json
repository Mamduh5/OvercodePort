[
  {
    "table": "admins",
    "schema": {
      "admin_id": {
        "type": "increments",
        "primary": true
      },
      "first_name": {
        "type": "string",
        "length": 50,
        "notNullable": false
      },
      "last_name": {
        "type": "string",
        "length": 50,
        "notNullable": false
      },
      "email": {
        "type": "string",
        "length": 50,
        "unique": true,
        "notNullable": true
      },
      "email_verification": {
        "type": "integer",
        "comment": "0 = UNVERIFIED, 1 = VERIFIED",
        "querystring": "CASE WHEN ${column1} = 0 THEN 'UNVERIFIED' WHEN ${column2} = 1 THEN 'VERIFIED' ELSE 'UNKNOWN' END AS email_verification",
        "default": 0,
        "notNullable": true
      },
      "is_2fa_active": {
        "type": "integer",
        "comment": "102 = ACTIVE, 402 = INACTIVE",
        "querystring": "CASE WHEN ${column1} = 1 THEN 'ACTIVE' WHEN ${column2} = 0 THEN 'INACTIVE' ELSE 'UNKNOWN' END AS is_2fa_active",
        "default": 0,
        "notNullable": true
      },
      "password_hash": {
        "type": "string",
        "length": 255,
        "notNullable": true
      },
      "refresh_token": {
        "type": "string",
        "length": 128,
        "notNullable": true
      }
    },
